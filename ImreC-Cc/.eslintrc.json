{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "no-console": 0,
    "strict": ["error", "global"],
    "no-unused-vars": ["error", {"vars": "all", "args": "none"}],
    "quotes": ["error", "single", {"avoidEscape": true, "allowTemplateLiterals": true}],
    "eqeqeq": ["error"],
    "func-style": ["error", "expression"]
  }
}